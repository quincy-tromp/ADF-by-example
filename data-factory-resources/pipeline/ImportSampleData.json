{
	"name": "ImportSampleData",
	"properties": {
		"description": "Import blob data from 'quincytrompsa' storage account into mystorageaccsa' storage account",
		"activities": [
			{
				"name": "Copy_xmp",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "sampledata/azure-data-factory-by-example-main/SampleData/SweetTreats/"
					},
					{
						"name": "Destination",
						"value": "myblobs//"
					}
				],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"wildcardFileName": "*"
						},
						"formatSettings": {
							"type": "BinaryReadSettings"
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						}
					},
					"enableStaging": false,
					"enableSkipIncompatibleRow": false,
					"skipErrorFile": {
						"fileMissing": true
					},
					"validateDataConsistency": false
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_xmp",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_xmp",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}